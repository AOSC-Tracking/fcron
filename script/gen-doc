#!/bin/sh

# $Id: gen-doc,v 1.6 2000-05-30 19:23:22 thib Exp $

PREVIOUS=`cat ./VERSION`


if test $PREVIOUS != $1; then
    cd doc
    for i in *
    do
    if test $i != RCS; then
	echo $i
	sed -e "s:fcron version [0-9.]*:fcron version $1:" < $i > tmp
	mv -f tmp $i
	chown $USER $i
	rm -f tmp
    fi
    done    
fi


